{
  "name": "jisho",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "initdb": "node ./scripts/init_db.js",
    "initex": "node ./scripts/examples_db.js",
    "initkanji": "node ./scripts/kanji_db.js",
    "db:create": "tsx scripts/migrate.ts --create",
    "db:import": "tsx scripts/migrate.ts --import",
    "db:import:words": "tsx scripts/migrate.ts --import-words",
    "db:reset": "tsx scripts/migrate.ts --reset",
    "db:stats": "tsx scripts/migrate.ts --stats",
    "db:verify": "tsx scripts/migrate.ts --verify",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:search": "jest __tests__/search.test.ts --config jest.config.node.js --watchAll=false --verbose",
    "test:dictionary": "jest __tests__/dictionary.test.ts --config jest.config.node.js --watchAll=false --verbose",
    "test:bookmarks": "jest __tests__/bookmarks.test.ts --config jest.config.node.js --watchAll=false --verbose",
    "test:history": "jest __tests__/history.test.ts --config jest.config.node.js --watchAll=false --verbose",
    "test:kanji": "jest __tests__/kanji.test.ts --config jest.config.node.js --watchAll=false --verbose",
    "test:utils": "jest __tests__/database-utils.test.ts --config jest.config.node.js --watchAll=false --verbose",
    "test:db": "jest __tests__ --config jest.config.node.js --watchAll=false --verbose",
    "test:ci": "jest --watchAll=false --verbose",
    "lint": "expo lint",
    "build": "eas build --platform ios",
    "build:preview": "eas build -p ios --profile preview"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@expo/metro-runtime": "^6.1.2",
    "@expo/ui": "~0.2.0-beta.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-ai/apple": "^0.9.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.4.1",
    "@react-navigation/native": "^7.1.6",
    "@shopify/flash-list": "2.0.2",
    "@tanstack/react-query": "5.89.0",
    "ai": "^5.0.44",
    "eventsource-parser": "^1.1.2",
    "expo": "54.0.8",
    "expo-audio": "~1.0.12",
    "expo-blur": "~15.0.7",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-network": "~8.0.7",
    "expo-router": "~6.0.6",
    "expo-speech": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-markdown-display": "^7.0.2",
    "react-native-mmkv": "^3.3.1",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-uitextview": "^2.1.0-rc.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-worklets": "0.5.1",
    "tiny-segmenter": "^0.2.0",
    "wanakana": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@tanstack/eslint-plugin-query": "^5.89.0",
    "@types/better-sqlite3": "7.6.13",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.10",
    "better-sqlite3": "12.2.0",
    "csv-parser": "^3.2.0",
    "eslint": "^9.33.0",
    "eslint-config-expo": "~10.0.0",
    "iconv-lite": "^0.6.3",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.11",
    "react-test-renderer": "19.1.0",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.4.1",
    "ts-node": "10.9.2",
    "tsx": "^4.20.3",
    "typescript": "~5.9.2"
  },
  "private": true,
  "packageManager": "yarn@4.9.4"
}
