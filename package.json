{
  "name": "jisho",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "initdb": "node ./scripts/init_db.js",
    "initex": "node ./scripts/examples_db.js",
    "initkanji": "node ./scripts/kanji_db.js",
    "db:create": "tsx scripts/migrate.ts --create",
    "db:import": "tsx scripts/migrate.ts --import",
    "db:import:words": "tsx scripts/migrate.ts --import-words",
    "db:reset": "tsx scripts/migrate.ts --reset",
    "db:stats": "tsx scripts/migrate.ts --stats",
    "db:verify": "tsx scripts/migrate.ts --verify",
    "frequency:update": "node scripts/update-frequency-scores.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:search": "jest __tests__/search.test.ts --config jest.config.node.js --watchAll=false --verbose",
    "test:dictionary": "jest __tests__/dictionary.test.ts --config jest.config.node.js --watchAll=false --verbose",
    "test:bookmarks": "jest __tests__/bookmarks.test.ts --config jest.config.node.js --watchAll=false --verbose",
    "test:history": "jest __tests__/history.test.ts --config jest.config.node.js --watchAll=false --verbose",
    "test:kanji": "jest __tests__/kanji.test.ts --config jest.config.node.js --watchAll=false --verbose",
    "test:utils": "jest __tests__/database-utils.test.ts --config jest.config.node.js --watchAll=false --verbose",
    "test:db": "jest __tests__ --config jest.config.node.js --watchAll=false --verbose",
    "test:ci": "jest --watchAll=false --verbose",
    "lint": "expo lint",
    "build": "eas build --platform ios",
    "build:preview": "eas build -p ios --profile preview"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.4.1",
    "@react-navigation/native": "^7.1.6",
    "@shopify/flash-list": "1.7.6",
    "@tanstack/react-query": "5.83.0",
    "expo": "53.0.20",
    "expo-audio": "~0.4.8",
    "expo-blur": "~14.1.5",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linking": "~7.1.7",
    "expo-network": "~7.1.5",
    "expo-router": "~5.1.3",
    "expo-speech": "~13.1.7",
    "expo-splash-screen": "~0.30.10",
    "expo-sqlite": "~15.2.14",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-context-menu-view": "^1.19.0",
    "react-native-executorch": "^0.4.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keyboard-controller": "^1.17.5",
    "react-native-markdown-display": "^7.0.2",
    "react-native-mmkv": "^3.3.0",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-uitextview": "^2.1.0-rc.0",
    "tiny-segmenter": "^0.2.0",
    "wanakana": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@types/better-sqlite3": "7.6.13",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "better-sqlite3": "12.2.0",
    "csv-parser": "^3.2.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~9.2.0",
    "iconv-lite": "^0.6.3",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.9",
    "react-test-renderer": "19.1.0",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.2",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3"
  },
  "private": true,
  "packageManager": "yarn@4.9.2"
}
