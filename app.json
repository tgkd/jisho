{
  "expo": {
    "name": "jisho",
    "slug": "jisho",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "ios": {
        "bundleIdentifier": "app.jisho.loc",
        "buildNumber": "3"
      },
      "supportsTablet": false,
      "bundleIdentifier": "app.jisho.loc",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.jisho.loc"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", ["expo-splash-screen"], "expo-sqlite"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0b8551c1-ee23-4d53-83b4-3849b88863ce"
      }
    }
  }
}
